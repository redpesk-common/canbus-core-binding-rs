{
  "name": "afb-canbus",
  "no-ldpaths": true,
  "port": 1234,
  "alias": [
    "/devtools:/usr/share/afb-ui-devtools/binder"
  ],
  "monitoring": true,
  "binding": [
    {
      "uid": "sockbcm",
      "path": "${CARGO_TARGET_DIR}/debug/libafb_sockcan.so",
      "info": "RUST sockbcm/can APIs",
      "sock_api": "sockcan",
      "sock_evt": "sockbcm"
    },
    {
      "uid": "bms",
      "path": "${CARGO_TARGET_DIR}/debug/libafb_bms.so",
      "info": "RUST bms/DBC API",
      "dbc_api": "bms",
      "sock_api": "sockbcm",
      "sock_evt": "sockcan"
    }
  ]
}
