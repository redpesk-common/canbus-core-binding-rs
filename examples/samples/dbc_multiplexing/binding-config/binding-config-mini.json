{
    "name": "afb-canbus-multiplexing",
    "no-ldpaths": true,
    "port": 1234,
    "alias": [
        "/devtools:/usr/share/afb-ui-devtools/binder"
    ],
    "monitoring": true,
    "binding": [
        {
            "uid": "sockbcm",
            "path": "${CARGO_TARGET_DIR}/debug/libafb_sockcan.so",
            "info": "RUST sockbcm/can APIs",
            "sock_api": "sockcan",
            "sock_evt": "sockbcm"
        },
        {
            "uid": "can-multiplexing",
            "path": "${CARGO_TARGET_DIR}/debug/libafb_can_multiplexing.so",
            "info": "RUST multiplexing/DBC API",
            "sock_api": "sockbcm",
            "sock_evt": "sockcan"
        }
    ]
}
