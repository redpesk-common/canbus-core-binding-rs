{
  "name": "afb-test",
  "no-ldpaths": true,
  "alias": [
    "/devtools:/usr/share/afb-ui-devtools/binder"
  ],
  "threads-max": 10,
  "monitoring": true,
  "binding": [
    {
      "uid": "sockbmc-binding",
      "path": "${CARGO_TARGET_DIR}/debug/libafb_sockbmc.so",
      "info": "RUST sockbmc/DBC APIs"
    },
    {
      "uid": "tap-test-binding",
      "path": "${CARGO_TARGET_DIR}/debug/libtap_sockbmc.so",
      "info": "Rust sockbmc API TAP/Test"
    }
  ],
  "set": {
    "tap-test": {
      "autostart": false,
      "autoexit": false,
      "output": "tap"
    }
  }
}
