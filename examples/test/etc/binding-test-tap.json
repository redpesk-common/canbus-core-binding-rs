{
  "name": "afb-sockbms-test",
  "no-ldpaths": true,
  "alias": [
    "/devtools:/usr/share/afb-ui-devtools/binder"
  ],
  "no_httpd": true,
  "trace-api": "all",
  "trace-req": "all",
  "trace-evt": "all",
  "binding": [
    {
      "uid": "sockcan",
      "path": "${CARGO_TARGET_DIR}/debug/libafb_sockcan.so",
      "info": "CAN raw socket service"
    },
    {
      "uid": "sockbms-binding",
      "path": "${CARGO_TARGET_DIR}/debug/libafb_bms.so",
      "require": [
        "sockcan"
      ],
      "info": "RUST sockbmc/DBC APIs",
      "args": {
        "dev": "vcan0",
        "uid": "sockbms",
        "sock_api": "sockcan",
        "sock_evt": "bmc-event"
      }
    }
  ]
}
