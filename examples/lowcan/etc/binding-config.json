{
  "name": "afb-sockcan",
  "no-ldpaths": true,
  "port": 1234,
  "alias": [
    "/devtools:/usr/share/afb-ui-devtools/binder"
  ],
  "monitoring": true,
  "binding": [
    {
      "uid": "sockcan-api",
      "path": "${CARGO_TARGET_DIR}/debug/libafb_sockcan.so",
      "info": "sockcan hardware abstraction",
      "dev": "vcan0",
      "api": "sockcan",
      "acls": "acl:vcan"
    }
  ]
}